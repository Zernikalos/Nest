{
  "name": "@zernikalos/nestserver",
  "version": "0.0.1",
  "description": "Zernikalos Nest Server",
  "private": true,
  "author": "Aarón Negrín <demenus.dev@gmail.com>",
  "license": "GPLv3",
  "types": "./dts/main.d.ts",
  "main": "./dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nest build",
    "prebuild": "rimraf dist dts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nest:build": "nest build",
    "dev": "START_SERVER=true nest start --watch -c nest-cli.json",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "START_SERVER=true node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs/common": "11.0.21",
    "@nestjs/core": "11.0.21",
    "@nestjs/platform-express": "11.0.21",
    "@nestjs/websockets": "11.0.21",
    "@nestjs/platform-ws": "11.0.21",
    "@nestjs/config": "4.0.2",
    "@nestjs/mapped-types": "2.1.0",
    "path-to-regexp": "8.2.0",
    "ws": "8.18.2",
    "rxjs": "7.8.2",
    "crc-32": "1.2.2",
    "class-validator": "0.14.2",
    "class-transformer": "0.5.1",
    "strip-ansi": "7.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.7",
    "@nestjs/schematics": "11.0.5",
    "@nestjs/testing": "11.0.21",
    "@types/supertest": "6.0.3",
    "source-map-support": "0.5.21",
    "supertest": "7.1.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "reflect-metadata": "0.2.2",
    "chokidar": "4.0.3"
  }
}
