{
  "name": "@zernikalos/zernikalos-studio",
  "version": "0.0.1",
  "description": "Zernikalos Studio Quasar Frontend",
  "productName": "Studio Quasar App",
  "author": "Aarón Negrín <demenus.dev@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"No test specified\" && exit 0",
    "start": "pnpm start:electron",
    "start:browser": "quasar dev",
    "start:electron": "quasar dev --mode electron",
    "build": "quasar build",
    "build:electron": "quasar build --mode electron",

    "prebuild": "rimraf dist",
    "build:nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:nest": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:nest": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:nest": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@zernikalos/zkbuilder": "0.0.1",
    "@quasar/extras": "1.16.4",
    "axios": "1.4.0",
    "follow-redirects": "1.15.2",
    "form-data": "4.0.0",
    "pinia": "2.1.4",
    "vue-demi": "0.14.5",
    "quasar": "2.12.2",
    "vue": "3.3.4",
    "vue-i18n": "9.2.2",
    "vue-router": "4.2.3",
    "bootstrap-icons": "1.10.5",
    "@nestjs/common": "10.0.5",
    "@nestjs/core": "10.0.5",
    "@nestjs/platform-express": "10.0.5",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "crc-32": "1.2.2",
    "class-validator": "0.14.0",
    "class-transformer": "0.5.1",
    "monaco-editor": "0.39.0",
    "lodash": "4.17.21",
    "@electron/remote": "2.0.10"
  },
  "devDependencies": {
    "electron": "25.2.0",
    "@quasar/app-vite": "2.0.0-alpha.2",
    "@types/node": "20.3.3",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "autoprefixer": "10.4.14",
    "electron-builder": "24.4.0",
    "eslint": "8.44.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-vue": "9.15.1",
    "typescript": "5.1.6",
    "@nestjs/cli": "10.1.7",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/supertest": "2.0.12",
    "jest": "29.6.0",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "webpack": "5.88.1",
    "rimraf": "5.0.1",
    "vite": "2.9.16",
    "postcss": "8.4.24",
    "vite-plugin-monaco-editor": "1.1.0"
  },
  "engines": {
    "node": "^18 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
